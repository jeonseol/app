        "php": ">=7.4",

        "slim/slim": "^4.7",

        "slim/twig-view": "^3",
        "slim/csrf": "^1",

        "slim/http": "^1.2",
        "nyholm/psr7": "^1.3",
        "nyholm/psr7-server": "^1",

        "php-di/php-di": "^6.3",
        "doctrine/annotations": "^1.11",
        "vlucas/phpdotenv": "^5.2",

        "monolog/monolog": "^2",
        "selective/basepath": "^2",
        "adbario/php-dot-notation": "^2.2"

    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "config/app.php"
        ]
    },
    "autoload-dev": {},
    "scripts": {}
